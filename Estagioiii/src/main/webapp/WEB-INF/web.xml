        <servlet-class>br.com.estagioiii.servelet.InsereRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MontaQuestionario</servlet-name>
        <servlet-class>br.com.estagioiii.servelet.MontaQuestionario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MultiplaEscolha</servlet-name>
        <servlet-class>br.com.estagioiii.servelet.MultiplaEscolha</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Texto</servlet-name>
        <servlet-class>br.com.estagioiii.servelet.Texto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CaixaCombinacao</servlet-name>
        <servlet-class>br.com.estagioiii.servelet.CaixaCombinacao</servlet-class>
