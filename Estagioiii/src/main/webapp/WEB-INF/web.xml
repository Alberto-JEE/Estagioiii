        <servlet-name>EditaQuestionario</servlet-name>
        <url-pattern>/Edita</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalvaEdicaoQuestionario</servlet-name>
        <url-pattern>/SalvaEdicao</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
